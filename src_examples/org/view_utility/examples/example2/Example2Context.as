package org.view_utility.examples.example2{	import flash.display.DisplayObjectContainer;	import org.osflash.signals.Signal;	import org.robotlegs.mvcs.ViewSignalContext;	import org.view_utility.examples.example2.commands.ProjectSetupCommand;	import org.view_utility.examples.example2.views.BackgroundView;	import org.view_utility.examples.example2.views.start.StartView;	/**	 * @author Peder A. Nielsen	 */	public class Example2Context extends ViewSignalContext	{		public function Example2Context(contextView:DisplayObjectContainer = null, autoStartup:Boolean = true)		{			super(contextView, autoStartup);		}				override public function startup():void		{			//map setup command and fire the signal to instatiate a new command object			var setupSignal:Signal = new Signal();			signalCommandMap.mapSignal(setupSignal, ProjectSetupCommand, true);			setupSignal.dispatch();						//add permanent views			addView(BackgroundView);			//add starting view			addView(StartView);		}	}}