package org.view_utility.examples.example3{	import flash.display.DisplayObject;	import org.robotlegs.core.IComponentView;	import org.robotlegs.mvcs.ViewSignalContext;	import flash.display.DisplayObjectContainer;	/**	 * @author Peder A. Nielsen	 */	public class Example3Context extends ViewSignalContext	{		public function Example3Context(contextView:DisplayObjectContainer = null, autoStartup:Boolean = true)		{			super(contextView, autoStartup);		}				override public function startup():void		{			//map mediator			mediatorMap.mapView(Example3View, Example3ViewMediator, IComponentView);						//add view			contextView.addChild(new Example3View());			//or simpler way			//addView(Example3View);		}	}}